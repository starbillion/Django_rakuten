# http://doc.scrapy.org/en/latest/topics/items.html

import scrapy

class WebscraperDemoItem(scrapy.Item):
    # define the fields for your item here like:
    all_product_count = scrapy.Field()
    find_product_count = scrapy.Field()
    fail_product_count = scrapy.Field()

    productId = scrapy.Field()
    product_name = scrapy.Field()
    category_name = scrapy.Field()
    prodURL = scrapy.Field()
    category_set_num = scrapy.Field()
    category_set_name = scrapy.Field()
    brand_name = scrapy.Field()
    price = scrapy.Field()
    prodDescription = scrapy.Field()
    prodImgURL = scrapy.Field()
    selectedColor = scrapy.Field()
    balance = scrapy.Field()
    selType = scrapy.Field()
    select_list = scrapy.Field()

    image_urls = scrapy.Field()
    images = scrapy.Field()